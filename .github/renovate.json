{
    "extends": [
        "config:base",
        ":disableDependencyDashboard"
    ],
    "ignorePaths": [
        "bigtable/api/**",
        "endpoints/getting-started/**"
    ],
    "packageRules": [
        {
            "matchPackagePrefixes": [
                "Microsoft.AspNetCore.",
                "mcr.microsoft.com/dotnet/"
            ],
            "allowedVersions": "<9.0.0"
        }
    ],
    "schedule": [
        "before 8am"
    ],
    "timezone": "Europe/London"
}
