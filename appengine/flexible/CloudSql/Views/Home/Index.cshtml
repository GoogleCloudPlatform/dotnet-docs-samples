@{
    ViewData["Title"] = "Google Cloud Sql Sample - Visitor Log";
}
@model CloudSql.ViewModels.HomeModel     

<H2>Visitor Log</H2>
<table>
    <thead><tr><td>Time Stamp<td>Ip Address</tr></thead>
    <tbody>
    @foreach (CloudSql.ViewModels.VisitorLogEntry entry in Model.VisitorLog) {
        <tr><td>@entry.TimeStamp<td>@entry.IpAddress</tr>
    }
    </tbody>
</table>