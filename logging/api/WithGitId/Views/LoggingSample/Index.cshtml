    @section Scripts{
      <script src="http://code.angularjs.org/1.2.13/angular.min.js"></script>
      <script src="~/js/main.js"></script>
    }

    <section ng-app="myApp" ng-controller="mainCtrl">
        <h3>Enter a count, click submit to generate log item samples.</h3>
        <p></p>
        <form ng-submit="doLogging()">
            <label for="LogCount">count</label>
            <input type="number" min="1" max="100" id="LogCount" ng-model="logCount">
            <input type="submit" />
        </form>
        <p>{{submitResult}}</p>
    </section>

